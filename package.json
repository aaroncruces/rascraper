{
  "name": "rascraper",
  "version": "1.0.0",
  "description": "scraper for retroarch and other systems",
  "main": "app.js",
  "type": "commonjs",
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "modulePathIgnorePatterns": [
      "fsSandbox"
    ],
    "globals": {
      "ts-jest": {
        "isolatedModules": true
      }
    }
  },
  "scripts": {
    "start": "ts-node-dev --transpile-only src/main/main.ts",
    "tsnode": "ts-node-dev --respawn --transpile-only --watch ./ src/main/main.ts",
    "tdd": "jest --watchAll  --runInBand ",
    "gitlog": "git log --graph --full-history --all --pretty=format:\"%h%x09%d%x20%s\""
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.26.0",
    "crc32": "^0.2.2",
    "electron": "^17.0.0",
    "minimist": "^1.2.5"
  },
  "Commentaries": [
    "put here commentaries about this file"
  ],
  "devDependencies": {
    "@tsconfig/node16": "^1.0.2",
    "@types/jest": "^27.4.0",
    "jest": "^27.5.1",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.5.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.5.5"
  }
}
